    "@rollup/plugin-node-resolve": "^8.4.0",
    "babel-loader": "^8.2.2",
    "better-docs": "^2.3.2",
    "json2module": "0.0",
    "rimraf": "^2.7.1",
    "rollup": "^2.29.0",
    "rollup-plugin-jsdoc": "^0.1.2",
